{
  "name": "cardmaster",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "dev": "concurrently \"npm run start\" \"npm run server:dev\"",
    "banks:upload": "tsx scripts/upload-banks-to-firestore.ts",
    "banks:upload:clean": "tsx scripts/upload-banks-to-firestore.ts --clean",
    "banks:upload:split": "tsx scripts/upload-banks-to-firestore.ts --split-by-track",
    "banks:upload:split:clean": "tsx scripts/upload-banks-to-firestore.ts --split-by-track --clean",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "web:build": "expo export -p web",
    "web:build:ghpages": "expo export -p web",
    "lint": "expo lint",
    "server:dev": "npm --prefix server run dev",
    "server:build": "npm --prefix server install && npm --prefix server run build",
    "server:start": "npm --prefix server run start",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.33",
    "expo-constants": "~18.0.13",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.23",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "firebase": "^12.10.0",
    "nativewind": "^4.1.23",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "firebase-admin": "^13.1.0",
    "patch-package": "^8.0.1",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.5",
    "typescript": "~5.9.2"
  },
  "private": true
}
