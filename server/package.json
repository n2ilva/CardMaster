{
  "name": "cardmaster-api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "prisma generate && tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "seed": "tsx src/seed.ts",
    "seed:dev-cards": "tsx src/seed-dev-ready-cards.ts",
    "seed:ml-cards": "tsx src/seed-ml-ready-cards.ts",
    "seed:infra-cloud-cards": "tsx src/seed-infra-cloud-ready-cards.ts",
    "seed:all-cards": "npm run seed:dev-cards && npm run seed:ml-cards && npm run seed:infra-cloud-cards"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.10",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.5.0",
    "typescript": "^5.8.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "tsx": "^4.19.3"
  }
}
